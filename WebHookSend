	HoneyDataV2.Text = game:GetService("Players").LocalPlayer.CoreStats.Honey.value
	CapacityDataV2.Text = game:GetService("Players").LocalPlayer.CoreStats.Capacity.value
	HealthDataV2.Text = game.Players.LocalPlayer.Character.Humanoid.Health

end)
end)
end)
