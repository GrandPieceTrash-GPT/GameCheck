repeat wait()
print("Waiting For Game To Load")
until game:IsLoaded()
if game.Players.LocalPlayer.PlayerGui:FindFirstChild("WebHookSendGui") then
   game.Players.LocalPlayer.PlayerGui.WebHookSendGui:Destroy()
end

